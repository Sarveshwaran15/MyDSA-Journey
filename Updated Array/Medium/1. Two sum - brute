public class TwoSum {
    public static void main(String[] args) {
        int[] arr={2,6,5,8,11};
        int target=14;
        int num=0;
        boolean result=false;
        for(int i=0;i<arr.length-1;i++)
        {
            num=arr[i];
            for(int j=i+1;j<arr.length;j++)
            {
                if(arr[j]+num==target)
                {
                    result=true;
                    break;
                }
            }
        }
        System.out.println(result);
    }
}
