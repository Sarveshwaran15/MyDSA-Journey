import java.util.HashSet;

public class Union {
    public static void main(String[] args) {
        int[] arr1={1,1,2,3,4,5};
        int[] arr2={2,3,4,4,5};
        HashSet<Integer> set=new HashSet<>();
        for(int n:arr1)
        {
            set.add(n);
        }
        for(int n1:arr2)
        {
            set.add(n1);
        }
       System.out.println(set);
    }
}
