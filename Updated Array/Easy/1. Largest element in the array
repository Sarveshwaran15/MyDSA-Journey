public class LargestAmongArray {
    public static void main(String[] args) {
        int[] arr={3,2,1,5,2};
        int largest=arr[0];
        for(int i=1;i<arr.length;i++)
        {
            if(largest<arr[i])
                largest=arr[i];
        }
        System.out.println(largest);
    }
}
