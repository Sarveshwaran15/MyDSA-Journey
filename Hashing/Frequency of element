public class Freqency {

    public static void main(String[] args) {
        int[] arr={1,2,3,5,4,5,6,5,7};
        int val=freq(5,arr);
        System.out.println(val);
    }
   static int freq(int element,int[] arr)
    {
        int count=0;
        for(int i=0;i<arr.length;i++)
        {
            if(arr[i]==element)
                count=count+1;
        }
        return count;
    }
}
